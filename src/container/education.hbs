<div>
 {{#with main}}
  <main>
    <section>
		 {{> title data="My educationss" big=true }}
      <ul>
		{{#each education}}
        <li>
          {{name}}
          {{year}}
          {{#unless isEnd}}
          (Навчаюсь)
          {{/unless}}
        </li>
		{{/each}}
      </ul>
    </section>
    <br>
    <section>
      {{> title data="My certificates" }}
      <ul>
    {{#each certificates}}
        <li>
          {{name}}
          ({{id}})
        </li>
        {{/each}}
      </ul>
    </section>
  </main>
  {{/with}}
</div>